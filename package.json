{
  "name": "sendmail2",
  "version": "0.1.0",
  "description": "Send Mail to Somewhere You Want",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": "./lib/index.js"
  },
  "files": [
    "/lib",
    "/types"
  ],
  "scripts": {
    "build": "tsc",
    "prepack": "rm -rf lib types && tsc && prettier --write \"types/**/*.d.ts\" \"lib/**/*.js\"",
    "format": "prettier --write \"src/**/*.ts\" \"./**/*.json\"",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "eslint --ext .ts src/ --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "nodemailer": "^6.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/node": "^17.0.30",
    "@types/nodemailer": "^6.4.4",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^7.0.4",
    "markdownlint-cli": "^0.31.1",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  },
  "keywords": [
    "send",
    "mail",
    "smtp",
    "email",
    "mailer",
    "sendmail",
    "nodemailer",
    "send mail",
    "send email"
  ],
  "author": "2020leon <leon2000.tw@gmail.com>",
  "homepage": "https://github.com/2020leon/sendmail2",
  "repository": "github:2020leon/sendmail2",
  "license": "MIT"
}
